{
  "name": "domainflow-frontend",
  "version": "1.0.0",
  "description": "DomainFlow - Enterprise Domain Analysis & Campaign Management Platform Frontend",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "next build --turbo",
    "build:safe": "npm run typecheck && npm run build",
    "start": "next start",
    "backend:dev": "cd backend && go run ./cmd/apiserver",
    "backend:build": "cd backend && go build -o bin/apiserver ./cmd/apiserver",
    "backend:start": "cd backend && ./bin/apiserver",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
  "issues:create": "node scripts/create_github_issues.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "perf:audit": "node scripts/performance-audit.mjs",
    "perf:budget": "node scripts/performance-budget.mjs",
    "size-limit": "size-limit",
    "api:bundle": "bash scripts/bundle-openapi-spec.sh",
    "api:generate-spec": "npm run api:bundle && echo 'Bundled to backend/openapi/dist/openapi.yaml'",
    "api:generate": "npm run gen:types && npm run gen:clients && npm run gen:docs && bash scripts/post-generate-cleanup.sh",
    "gen:openapi": "echo 'Spec is modular (root: backend/openapi/openapi.root.yaml, bundle: backend/openapi/dist/openapi.yaml)'",
    "gen:types": "openapi-typescript backend/openapi/dist/openapi.yaml -o src/lib/api-client/types.ts",
    "gen:clients": "openapi-generator-cli generate -i backend/openapi/dist/openapi.yaml -g typescript-axios -o src/lib/api-client --additional-properties=supportsES6=true,withInterfaces=true,typescriptThreePlus=true,withSeparateModelsAndApi=true,apiPackage=apis,modelPackage=models,stringEnums=true,enumPropertyNaming=original,generateAliasAsModel=true",
    "gen:docs": "openapi-generator-cli generate -i backend/openapi/dist/openapi.yaml -g html2 -o src/lib/api-client/docs",
    "gen:all": "npm run gen:types && npm run gen:clients && npm run gen:docs && bash scripts/post-generate-cleanup.sh",
    "api:validate": "bash scripts/validate-openapi-spec.sh",
    "api:validate-spec": "bash scripts/validate-openapi-spec.sh",
    "api:validate-bundle": "SPEC=backend/openapi/dist/openapi.yaml bash scripts/validate-openapi-spec.sh",
    "api:check-routes": "node scripts/check-routes.mjs",
    "api:autogen": "node scripts/api-autogen.mjs",
    "test:visual": "percy exec -- jest src/components/ui/__tests__/visual/",
    "percy:start": "percy exec -- start-server-and-test start http://localhost:8080 'npm run test:visual'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "playwright test tests/accessibility --reporter=html",
    "analyze": "cross-env ANALYZE=true next build",
    "lighthouse": "lhci autorun",
    "test:performance": "playwright test tests/performance",
    "test:campaign": "node scripts/e2e-campaign-test.js",
    "api:regen:quick": "npm run api:bundle && npm run gen:types && npm run gen:clients && bash scripts/post-generate-cleanup.sh",
  "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.66.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.514.0",
    "next": "^15.3.3",
    "puppeteer": "^24.11.2",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-redux": "^9.2.0",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.30.1",
    "@lhci/cli": "^0.14.0",
    "@next/bundle-analyzer": "^15.3.4",
    "@next/eslint-plugin-next": "^15.3.5",
    "@openapitools/openapi-generator-cli": "2.21.0",
    "@percy/cli": "^1.31.0",
    "@percy/playwright": "^1.0.8",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20.19.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "axe-playwright": "^2.1.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lighthouse": "^12.2.0",
    "openapi-typescript": "7.8.0",
    "playwright": "^1.54.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=20.19.0 <21",
    "npm": ">=10 <11"
  }
}
