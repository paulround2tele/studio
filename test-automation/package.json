{
  "name": "domainflow-browser-testing",
  "version": "1.0.0",
  "description": "Comprehensive browser testing suite for DomainFlow campaign system",
  "main": "comprehensive-browser-test.js",
  "scripts": {
    "test": "node comprehensive-browser-test.js",
    "test:headless": "HEADLESS=true node comprehensive-browser-test.js",
    "test:debug": "DEBUG=true node comprehensive-browser-test.js",
    "test:suite": "node domainflow-test-suite.js",
    "test:suite:headless": "node domainflow-test-suite.js --headless",
    "test:suite:debug": "node domainflow-test-suite.js --debug",
    "test:suite:full": "node domainflow-test-suite.js --capture-logs --max-campaigns=10"
  },
  "dependencies": {
    "puppeteer": "^21.6.1"
  },
  "keywords": [
    "browser-testing",
    "puppeteer",
    "automation",
    "websocket",
    "debugging"
