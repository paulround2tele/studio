{
    "IDX.aI.enableInlineCompletion": true,
    "files.exclude": {
        "**/.DS_Store": false,
        "**/.git": false,
        "**/.hg": false,
        "**/.svn": false,
        "**/*.crswap": false,
        "**/CVS": false,
        "**/Thumbs.db": false
    },
    "files.hotExit": "off",
    "files.restoreUndoStack": false,
    "workbench.editor.restoreViewState": false,
    "window.restoreWindows": "none",
    "search.exclude": {
        "**/*.code-search": false,
        "**/bower_components": false,
        "**/node_modules": false
    },
    "npm.exclude": "",
    "IDX.aI.enableCodebaseIndexing": true,
    "mcp.servers": {
        "studio-backend-context": {
            "enabled": true,
            "autoReconnect": true
        },
        "domainflow": {
            "enabled": true,
            "autoReconnect": true
        }
    },
    "chat.mcp.serverSampling": {
        "studio/.vscode/mcp.json: domainflow": {
            "allowedModels": [
                "github.copilot-chat/claude-sonnet-4",
                "github.copilot-chat/claude-opus-4"
            ]
        }
    }
}